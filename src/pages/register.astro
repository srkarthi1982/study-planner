---
import { buildRootRedirect, getSafeReturnTo } from "../lib/redirects";

return Astro.redirect(
  buildRootRedirect(
    Astro.locals.rootAppUrl ?? "https://ansiversa.com",
    "/register",
    getSafeReturnTo(Astro.url.searchParams.get("returnTo"), new URL("/", Astro.url).toString()),
  ),
);
